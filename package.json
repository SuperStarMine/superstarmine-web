{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "run-s build:necro build:svelte",
    "build:svelte": "rollup -c;tools/cache-busting.sh",
    "build:necro": "sass src/pages/newcromance/style/main.scss:src/pages/newcromance/style/style.css --style compressed",
    "dev": "run-p dev:necro dev:svelte",
    "dev:svelte": "rollup -c -w",
    "dev:necro": "sass --watch src/pages/newcromance/style/main.scss:src/pages/newcromance/style/style.css",
    "svelte-check": "svelte-check",
    "start": "sirv ./ --single --host",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "bezier-easing": "^2.1.0",
    "color": "^3.1.3",
    "hls.js": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.42.1",
    "sat": "^0.9.0",
    "sha256": "^0.2.0",
    "sirv-cli": "^1.0.0",
    "stylus": "^0.54.8",
    "supports-webp": "^2.0.1",
    "svelte": "^3.0.0",
    "svelte-check": "^1.2.3",
    "svelte-intersection-observer": "^0.8.0",
    "svelte-preprocess": "^4.6.7",
    "svelte-spa-router": "^3.1.0",
    "svelte-youtube": "^0.0.2",
    "swiper": "^6.5.3"
  },
  "dependencies": {
    "@splidejs/svelte-splide": "^0.2.9"
  }
}
