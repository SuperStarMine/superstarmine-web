{
  "name": "svelte-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "old:build": "rollup -c;tools/cache-busting.sh",
    "old:dev": "rollup -c -w",
    "old:svelte-check": "svelte-check",
    "old:start": "sirv ./ --single --host",
    "old:lint": "eslint . --ext .js,.svelte"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "^1.0.0-next.117",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "bezier-easing": "^2.1.0",
    "color": "^3.1.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "hls.js": "^1.0.4",
    "prettier": "^2.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sat": "^0.9.0",
    "sha256": "^0.2.0",
    "sirv-cli": "^1.0.0",
    "stylelint": "^13.13.1",
    "stylelint-plugin-stylus": "^0.11.0",
    "stylus": "^0.54.8",
    "supports-webp": "^2.0.1",
    "svelte": "^3.0.0",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.6.7",
    "svelte-spa-router": "^3.1.0",
    "svelte-youtube": "^0.0.2",
    "swiper": "^6.5.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  }
}
